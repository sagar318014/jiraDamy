@model List<ViewModel.LableListViewModel>
@{
    ViewBag.Title = "LableListView";
    Layout = "~/Views/Shared/_AuthorizedLayout.cshtml";

}
@section Grid{
    <div> <h1 style="font-weight:bold;text-align:center">Label List</h1></div>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <a href="@Url.Action("AddLable", "Home")" class="btn btn-success" style="float:right"> Add Lable </a>
    <div class="text-center">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Lable Name</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr style="text-align:center">
                        <td>@item.LableName</td>
                    </tr>
                }

            </tbody>

        </table>


    </div>

}

@section scripts{
    <script>


    </script>
}